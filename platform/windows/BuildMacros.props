<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <GS_ThirdPartyDir>$(SolutionDir)\..\..\thirdparty</GS_ThirdPartyDir>
    <GS_CodeDir>$(SolutionDir)\..\..\code</GS_CodeDir>
  </PropertyGroup>
  <PropertyGroup />
  <ItemDefinitionGroup />
  <ItemGroup>
    <BuildMacro Include="GS_ThirdPartyDir">
      <Value>$(GS_ThirdPartyDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
    <BuildMacro Include="GS_CodeDir">
      <Value>$(GS_CodeDir)</Value>
      <EnvironmentVariable>true</EnvironmentVariable>
    </BuildMacro>
  </ItemGroup>
</Project>